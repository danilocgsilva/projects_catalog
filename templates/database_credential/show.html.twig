{% extends 'base.html.twig' %}

{% block title %}DatabaseCredential{% endblock %}

{% block body %}
    <h1>DatabaseCredential</h1>

    <table class="table">
        <tbody>
            <tr>
                <th>Id</th>
                <td>{{ database_credential.id }}</td>
            </tr>
            <tr>
                <th>Name</th>
                <td>{{ database_credential.name }}</td>
            </tr>
            <tr>
                <th>DatabaseName</th>
                <td>{{ database_credential.databaseName }}</td>
            </tr>
            <tr>
                <th>Port</th>
                <td>{{ database_credential.port }}</td>
            </tr>
            <tr>
                <th>Host</th>
                <td>{{ database_credential.host }}</td>
            </tr>
            <tr>
                <th>Password</th>
                <td>{{ database_credential.password }}</td>
            </tr>
        </tbody>
    </table>

    <a href="{{ path('app_database_credential_index') }}">back to list</a>

    <a href="{{ path('app_database_credential_edit', {'id': database_credential.id}) }}">edit</a>

    {{ include('database_credential/_delete_form.html.twig') }}
{% endblock %}
